<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{HH:mm:ss.SSS} [%thread] %-5level %logger{36} - %msg%n %mdc</pattern>
        </encoder>
    </appender>

    <!-- Set logging levels -->
    <logger name="org.apache.catalina" level="TRACE"/>
    <logger name="gov.fnal.frontier" level="TRACE"/>

    <root level="INFO">
        <appender-ref ref="STDOUT"/>
    </root>
</configuration>
